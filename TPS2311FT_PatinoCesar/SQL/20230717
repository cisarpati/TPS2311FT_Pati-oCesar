INSERT INTO `platziblog`.`post` (`id`,`titulo`,`fecha_publicacio

SELECT estatus, SUM(id) suma_id
FROM posts
GROUP BY estatus;

SELECT estatus, COUNT(*) post_quantily
FROM posts
GROUP BY estatus;