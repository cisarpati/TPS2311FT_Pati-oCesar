CREATE schema `platziblog` default character set utf8mb4;

use `platziblog`

CREATE TABLE `platziblog`.`post`(
`id` INT NOT NULL AUTO_INCREMENT, 
`titulo` VARCHAR(100) NULL, 
`fecha_publicacion`datetime, 
`contenido`VARCHAR(3000), 
`estatus`VARCHAR(8),
`usuario_id` INT, 
`categoria_id` INT, 
PRIMARY KEY (`id`));

