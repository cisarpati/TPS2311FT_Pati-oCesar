SELECT *
FROM post
where fecha_publicacion> '2024-01-01';

select titulo,fecha_publicacion,estatus
from post;

select titulo AS encabezado,fecha_publicacion As publicado_en,estatus AS estado
from post;

select  count(*) AS numero_registros
from post;

SELECT * from platziblog.post;

CREATE TABLE `platziblog `.`cursos`(
`id` INT NOT NULL AUTO_INCREMENT, 
`nombre` VARCHAR(255) NULL, 
`profe` VARCHAR(255) null, 
`n_calificaciones` int null,  
PRIMARY KEY (`id`));

INSERT INTO `platziblog`.`cursos` (`id`, `nombre`, `profe`, `n_calificaciones`) VALUES (1, 'Curso de SQL', 'Israel', 115);
INSERT INTO `platziblog`.`cursos` (`id`, `nombre`, `profe`, `n_calificaciones`) VALUES (2, 'MySQL y MariaDB', 'Carlos', 180);
INSERT INTO `platziblog`.`cursos` (`id`, `nombre`, `profe`, `n_calificaciones`) VALUES (3, 'PostgreSQL', 'Oswaldo', 150);

SELECT *
FROM cursos
where id> '1';

select nombre,profe,n_calificaciones
from cursos;


CREATE TABLE `platziblog`.`usuarios`(
`id` INT NOT NULL AUTO_INCREMENT, 
`login` VARCHAR(255) NULL, 
`password`varchar(255) null, 
`nickname`VARCHAR(255) null, 
`email`VARCHAR(255) null,
PRIMARY KEY (`id`));

CREATE TABLE `platziblog`.`categorias`(
`id` INT NOT NULL AUTO_INCREMENT, 
`nombre_categoria` VARCHAR(255) NULL, 
PRIMARY KEY (`id`));

CREATE TABLE `platziblog`.`etiquetas`(
`id` INT NOT NULL AUTO_INCREMENT, 
`nombre_etiqueta` VARCHAR(255) NULL, 
PRIMARY KEY (`id`));

CREATE TABLE `platziblog`.`post`(
`id` INT NOT NULL AUTO_INCREMENT, 
`titulo` VARCHAR(100) NULL, 
`fecha_publicacion`datetime, 
`contenido`VARCHAR(3000), 
`estatus`VARCHAR(8),
`usuario_id` INT, 
`categoria_id` INT, 
PRIMARY KEY (`id`));

CREATE TABLE `platziblog`.`posts_etiquetas`(
`id` INT NOT NULL AUTO_INCREMENT, 
`post_id` VARCHAR(255) NULL, 
`etiqueta_id` VARCHAR(255) NULL,
PRIMARY KEY (`id`));

-- Estructura basica de un query
SELECT	*
FROM		posts;

-- Estructura extendida de un query
SELECT	*
FROM		posts
WHERE	fecha_publicacion > '2024';
